{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "float"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published"
      ],
      "default": "published",
      "required": true
    },
    "category_children": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category-child.category-child",
      "mappedBy": "products"
    },
    "OriginalPrice": {
      "type": "float"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Content": {
      "type": "dynamiczone",
      "components": [
        "content.ricktext",
        "content.video"
      ]
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "share.seo"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    }
  }
}
